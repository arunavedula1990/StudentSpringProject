<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    
    <bean id="student" class="com.mycompany.domain.Student" autowire="byType">
        <property name="firstName" value="bob" />        
        <property name="lastName" value="jones" /> 
        <property name="dateOfBirth" value="03/11/1986" /> 
    </bean>

    <bean id="address" class="com.mycompany.domain.Address" > 
        <property name="street" value="almeda st" />        
        <property name="city" value="houston" /> 
        <property name="zip" value="77054" />        
    </bean>
    
    <bean id="grade" class="com.mycompany.domain.Grade" >    
        <property name="subject" value="INEW 2438" />        
        <property name="letterGrade" value="A" />     
    </bean>
</beans>